<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/backgroundColor"
        tools:context=".MainActivity">


    <!-- Title -->


    <!-- Times clicked TextView -->


    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="40sp"
            android:text="@string/textName"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:id="@+id/textView"
            android:gravity="center"
            android:layout_marginBottom="556dp"
            app:layout_constraintHorizontal_bias="0.496"
            android:layout_marginTop="126dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageButton"
            tools:ignore="UnknownId"/>
    <TextView android:layout_width="106dp"
              android:layout_height="42dp"
              android:textSize="15sp"
              android:text="@string/credits"
              app:layout_constraintLeft_toLeftOf="parent"
              app:layout_constraintRight_toRightOf="parent"
              android:id="@+id/textView3"
              android:layout_marginBottom="16dp"
              android:gravity="center"
              app:layout_constraintBottom_toBottomOf="parent"
              app:layout_constraintHorizontal_bias="0.531"/>


    <!-- Amount of times clicked -->

    <!-- Goose image -->

    <!-- Goose image -->

    <TextView android:layout_width="134dp"
              android:layout_height="38dp"
              android:text="@string/amountName"
              android:textSize="20sp"
              android:gravity="center"
              android:id="@+id/textView2"
              app:layout_constraintTop_toBottomOf="@+id/textView" android:layout_marginBottom="399dp"
              app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.498"
              app:layout_constraintVertical_bias="0.449"/>
    <TextView android:layout_width="152dp"
              android:layout_height="66dp"
              android:text="@string/amountInt"
              android:textSize="40sp"
              android:gravity="center"
              android:id="@+id/textView5"
              app:layout_constraintTop_toBottomOf="@+id/textView" android:layout_marginBottom="399dp"
              app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.498"
              app:layout_constraintVertical_bias="1.0"/>
    <ImageButton android:layout_width="232dp"
                 android:layout_height="251dp"
                 android:scaleType="fitCenter"
                 android:paddingStart="2dp"
                 android:paddingEnd="2dp"
                 android:background="@color/backgroundColor"
                 android:paddingLeft="20dp"
                 android:paddingRight="20dp"
                 android:src="@drawable/h9m39vv630h51"
                 android:id="@+id/imageButton2"
                 app:layout_constraintTop_toBottomOf="@+id/textView2" android:layout_marginBottom="89dp"
                 app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                 app:layout_constraintBottom_toBottomOf="parent" tools:ignore="ContentDescription"
                 app:layout_constraintHorizontal_bias="0.413" app:layout_constraintVertical_bias="0.532"/>

    <ImageButton android:layout_width="65dp"
                 android:layout_height="61dp"
                 android:scaleType="fitCenter"
                 android:src="@drawable"
                 app:layout_constraintTop_toBottomOf="@+id/textView2" android:layout_marginBottom="89dp"
                 app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                 app:layout_constraintBottom_toBottomOf="parent" tools:ignore="ContentDescription"
                 app:layout_constraintHorizontal_bias="0.104" app:layout_constraintVertical_bias="0.0"
    />

</androidx.constraintlayout.widget.ConstraintLayout>